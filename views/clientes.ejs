    <h1>Clientes</h1>
    <%# Comentário no EJS %>

 
    <table border="1">
        <tr>
            <th width="150px">Nome:</th>
            <th>CPF:</th>
            <th>Endereço:</th>
        </tr>
            <% clientes.forEach((cliente)=>{ %>
        <tr>
            <td><%= cliente.nome %></td>
            <td><%= cliente.cpf %></td>         
            <td><%= cliente.endereco %></td>
        </tr>
            <% }) %>
    </table>

